- if @recordings.present?
  .col-md-12
    .headline
      %h2 Uploads
    .TileSliderWrapp.owlSlider
      .spinnerSlider
        .bounceS1
        .bounceS2
        .bounceS3
      .TileSlider.owl-carousel
        - @recordings.each do |video|
          = render_shared_model_with_live_views_counter('shared/tiles/recording', recording: video)
