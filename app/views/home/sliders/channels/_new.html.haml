- if @new_channels.present?
  .col-md-12
    .headline
      %h2 New
    .TileSliderWrapp.owlSlider
      .spinnerSlider
        .bounceS1
        .bounceS2
        .bounceS3
      .TileSlider.owl-carousel
        - @new_channels.each do |channel|
          = render('shared/tiles/channel', channel: channel)
