- if @most_popular_sessions_or_channels.present?
  .col-md-12
    .headline
      %h2
        Popular
    .TileSliderWrapp.owlSlider
      .spinnerSlider
        .bounceS1
        .bounceS2
        .bounceS3
      .TileSlider.owl-carousel
        = render(partial: 'shared/tiles/session', collection: @most_popular_sessions_or_channels, locals: {custom_class: 'margin-right-10'})
